<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id='user_reserve_action' model='ir.actions.act_window'>
        <field name="name">Car Reserve Driver</field>
        <field name="res_model">res.users</field>
        <field name="view_mode">tree</field>
    </record>



    <record id="user_reserve_view_tree" model="ir.ui.view">
        <field name="name">User List</field>
        <field name="model">car.history</field>

        <field name="arch" type="xml">
            <tree>

                <field name="name"/>
                <field name="reserved_from"/>
                <field name="reserved_to"/>
                <field name="num_of_day"/>


            </tree>
        </field>
    </record>


</odoo>